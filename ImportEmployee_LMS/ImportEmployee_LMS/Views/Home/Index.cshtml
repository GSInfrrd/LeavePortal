@{
    ViewBag.Title = "Home Page";
}
@model ImportEmployee_LMS.Models.EmployeeUploadModel

<div class="row">
    <div class="col-md-4">
        <h3>Upload employee data</h3>
        @using (Html.BeginForm("UploadEmployeeData", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            @Html.TextBoxFor(m => m.EmployeeFile, new { type = "file" })
            <button type="submit" class="btn btn-primary">Upload</button>
        }
    </div>
</div>
                                url: '/HR/GetCities',
                                data: { StateId: stateId},
                                //contentType: "application/json",
                                success: function (data) {
                                    for (var i = 0; i < data.data.length; i++) {
                                        $("#ddlPermanentAddressCity").append($('<option></option>').attr("value", data.data[i].Id).text(data.data[i].CityName));
                                    }
                                }
                            });
                        });

                        $('#ddlCurrentAddressCountry').on('change', function () {
                            if ($("#ddlCurrentAddressCountry").val() != 0) {
                                $("#divCurrentCountryError").hide();
                            }
                            else {
                                $("#divCurrentCountryError").show();
                            }
                        });

                        $('#ddlPermanentAddressCountry').on('change', function () {
                            if ($("#ddlPermanentAddressCountry").val() != 0) {
                                $("#divPermanentCountryError").hide();
                            }
                            else {
                                $("#divPermanentCountryError").show();
                            }
                        });
                    </script>

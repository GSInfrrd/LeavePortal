                                </div>
                                <div class="form-group">
                                    <label for="inputName" class="col-sm-2 control-label">Manager Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" readonly="readonly" class="form-control" value="@Model.ManagerName">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputName" class="col-sm-2 control-label">Project Name</label>
                                    <div class="col-sm-10">
                                        <input type="text" readonly="readonly" class="form-control" value="@Model.ProjectName">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputName" class="col-sm-2 control-label">Profile Type</label>
                                    <div class="col-sm-10">
                                        <input type="text" readonly="readonly" class="form-control" value="@Model.ProfileTypeName">
                                    </div>
                                </div>
                               
                                
                               
                               
                            </div>
                        </form>
                    </div>
                    <div class="tab-pane" id="emergencyContactDetails">
                        <form class="form-horizontal">
                            <div id="divemergencyContactInfo">
                                <h3>Emergency Contact Details</h3>
                                <div class="form-group">
                                    <label for="inputName" class="col-sm-2 control-label">Name</label>
                                    <div class="col-sm-10">
                                        <input type="hidden" class="form-control" id="txtEmpID" placeholder="First Name" value="@Model.Id">
                                        <input type="text" class="form-control" id="txtEmergencyContactName" value="@Model.EmployeeEmergencyContactDetail[0].Name">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputName" class="col-sm-2 control-label">Relationship</label>
                                    <div class="col-sm-10">
                                        <select class="form-control graduation" style="width: 100%;" id="ddlEmergencyContactRelationship">
                                            <option value="">Select</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputName" class="col-sm-2 control-label">Telephone</label>
                                    <div class="col-sm-10">

            && ($("#txtEmergencyContactName").val() == "") && ($("#ddlEmergencyContactRelationship").val() == 0) && ($("#txtEmergencyContactTelephone").val() == "") && ($("#ddlEmergencyContactCountry").val() == 0)
            && ($("#ddlPermanentCountry").val() == 0) && ($("#txtPermanentAddress").val() == "")
            && ($("#ddlCurrentCountry").val() == 0) && ($("#txtCurrentAddress").val() == "")
            && ($("#ddlFacility").val() == 0)
            && ($("#ddlGraduation").val() == 0)  && ($("#txtUniversity").val() == "")
            && ($("#txtEmployer").val() == "") && ($("#ddlExperienceDesignation").val() == 0) && ($("#ddlSkills").val()== null) && ($("#experienceDatePicker").val()== "")) {
            $("#divFirstNameError").show();
            $("#divLastNameError").show();
            $("#divEmailError").show();
            $("#divTelephoneError").show();
            $("#divGenderError").show();
            $("#divBirthDayError").show();
            $("#divEmployeeNumberError").show();
            $("#divInfrrdEmailError").show();
            $("#divDesignationError").show();
            $("#divEmployeeTypeError").show();
            $("#divEmployeeContractTypeError").show();
            $("#divManagerNameError").show();
            $("#divProjectNameError").show();
            $("#divEmergencyContactNameError").show();
            $("#divEmergencyContactRelationshipError").show();
            $("#divEmergencyContactTelephoneError").show();
            $("#divEmergencyContactCountryError").show();
            $("#divPermanentCountryError").show();
            $("#divPermanentAddressError").show();
            $("#divCurrentCountryError").show();
            $("#divCurrentAddressError").show();
            $("#divFacilityError").show();
            $("#divGraduationError").show();
            $("#divCollegeError").show();
            $("#divEmployerError").show();
            $("#divEmpDesignationError").show();
            $("#divSkillsError").show();
            $("#divFromError").show();
            $('#divInvalidEmailError').hide();
            $('#divInvalidInfrrdEmailError').hide();
            $('#divEmployeeNumberAlreadyError').hide();
            $('#divEmailAlreadyError').hide();
            $('#EmpPersonalInfoToggle').collapse('show');
            $("#txtFirstName").focus();
            
            return false;
        }

        if ($("#txtFirstName").val() == "") {
            $('#EmpPersonalInfoToggle').collapse('show');
            $('#divFirstNameError').show();
            $("#txtFirstName").focus();
            return false;
        }
        if ($("#txtLastName").val() == "") {
            $('#EmpPersonalInfoToggle').collapse('show');
            $('#divLastNameError').show();
            $("#txtLastName").focus();
            return false;
        }
        if ($("#txtEmail").val() == "") {
            $('#EmpPersonalInfoToggle').collapse('show');
            $('#divEmailError').show();
            $('#divInvalidEmailError').hide();
            $('#divEmailAlreadyError').hide();
            $("#txtEmail").focus();
            return false;
        }
        var reg = /^\w+([-+.']\w+)*@Html.Raw("@")\w+([-.]\w+)*\.\w+([-.]\w+)*$/
        if (!(reg.test($("#txtEmail").val()))) {
            $('#EmpPersonalInfoToggle').collapse('show');
            $('#divInvalidEmailError').show();
            $('#divEmailError').hide();
            $("#txtEmail").focus();
            return false;
        }
        else
        {
            $('#divInvalidEmailError').hide();
            $('#divEmailError').hide();
        }


        if ($("#txtTelephone").val() == "") {
            $('#EmpPersonalInfoToggle').collapse('show');
            $('#divTelephoneError').show();
            $("#txtTelephone").focus();

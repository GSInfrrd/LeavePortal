            $('#divInfrrdEmailError').show();
            $('#divInvalidInfrrdEmailError').hide();
            $("#txtInfrrdEmail").focus();
            return false;
        }
        var reg = /^\w+([-+.']\w+)*@Html.Raw("@")\w+([-.]\w+)*\.\w+([-.]\w+)*$/
